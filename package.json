{
  "name": "osu-practice-tool",
  "version": "0.0.2",
  "description": "A tool to quickly generate practice difficulties for osu!.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . osu-practice-tool --overwrite",
    "build:all": "electron-packager . osu-practice-tool --all"
  },
  "author": "Jerry Zhu",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.19",
    "archiver": "^3.0.0",
    "iohook": "^0.5.1"
  },
  "devDependencies": {
    "electron": "^5.0.7",
    "electron-packager": "^14.0.1"
  },
  "iohook": {
    "targets": [
      "node-57",
      "electron-70"
    ],
    "platforms": [
      "win32"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  }
}
